{
  "name": "playback",
  "version": "0.0.0",
  "description": "Playback microphone audio through speakers in realtime with adjustable gain",
  "main": "build/index.js",
  "scripts": {
    "start": "tsb serve",
    "build": "tsb build",
    "lint": "eslint '**/*.{ts,tsx,js,jsx}'",
    "format": "prettier --write '**/*'",
    "format-check": "prettier --check '**/*'",
    "typecheck": "tsc -v && tsc --noEmit",
    "test": "npm run typecheck && npm run format-check && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jakesidsmith/playback.git"
  },
  "keywords": [
    "microphone",
    "audio",
    "playback",
    "realtime",
    "web",
    "api",
    "speakers"
  ],
  "author": "Jake 'Sid' Smith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jakesidsmith/playback/issues"
  },
  "homepage": "https://github.com/jakesidsmith/playback#readme",
  "dependencies": {
    "@jakesidsmith/tsb": "^1.0.1",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8"
  }
}
